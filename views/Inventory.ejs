<!DOCTYPE html>
<head>
    <title>Inventory</title>
</head>
<body>
    <h1>Inventory</h1>
    <table>
        <thead>
            <tr>
                <th>Product ID</th>
                <th>Product Name</th>
                <th>Quantity</th>
            </tr>
        </thead>
        <tbody>
            <% for (var i in teammembers) {%>
            <tr>
                <td><%=teammembers[i].product_id%></td>
                <td><%=teammembers[i].product_name%></td>
                <td><%=teammembers[i].quantity%></td>
            </tr>
            <% } %>
        </tbody>
    </table>
</body>